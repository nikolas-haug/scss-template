{
  "name": "scss-template",
  "version": "1.0.0",
  "description": "a boilerplate for small or large front end projects",
  "main": "index.js",
  "scripts": {
    "compile:basic": "node-sass ./basic/main.scss ./basic/css/style.css --output-style compressed -w",
    "compile:advanced": "node-sass ./advanced/sass/main.scss ./advanced/css/style.css --output-style compressed -w",
    "dev": "node-sass ./basic/main.scss ./basic/css/style.css -w",
    "build": "node-sass ./basic/main.scss ./basic/css/style.css --output-style compressed -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolas-haug/scss-template.git"
  },
